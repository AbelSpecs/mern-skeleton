{
    "presets": [
        ["@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }                
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        [
            "styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false
            }
        ]
    ]
    
}